@charset "utf-8";
/* CSS Document */
body {
	/*font-size: 18.15px;*/
	line-height: 1.5;
	font-family: 'Roboto', sans-serif;
	font-weight: 400;
	color:#fff;
	-webkit-transition: all 0.4s;
    -moz-transition: all 0.4s;
	-o-transition: all 0.4s;
	transition: all 0.4s;
	background-image: url("pic/Front1.jpg"), url("pic/unendlich.jpg");
	background-repeat:no-repeat,repeat-y;
	background-position:center top;
}
a {
	color: #099acb;
	text-decoration:none;
}
#wrapper {
	margin:auto;
	width:1000px;
	margin-top:174px;
	height:100%
}
.logininput  { 
	font-size: 16px;
	font-weight:300;
	color: #707070;
	margin-bottom:3px;
	vertical-align: bottom;
	border:none;
	outline:none;
	background:none;
}
.hover:hover{

}
.login-border-bottom {
	width:20%;
	float:left;
	border-bottom:1px solid #CCC;
	margin-left:5px;
}
.logout {
	background:none;
	color: #099acb;
	text-decoration:none;
	border:none;
	font-size:16px;
	margin:0px;
	padding:0px;
	cursor: pointer;
}
.login {
	padding-top:5px;
}
.titleline{
	padding-top:16px;
	font-size:24px;
	font-weight:bold;
	height:15px;
	color:#ba9056;
	border:none;
	margin-bottom:30px;
}
.team {
	height:35px;
	width:30%;
	padding: 9px 0px;
	margin: 1px 0px;
	float:left;
	text-align:center;
	cursor: pointer;
}
.team1{
	width:30%;
	padding: 9px 0px;
	margin: 1px 0px;
	float:left;
	text-align:center;
	cursor: pointer;
	background-position:right !important;
}
.team2{
	width:30%;
	padding: 9px 0px;
	margin: 1px 0px;
	float:left;
	text-align:center;
	cursor: pointer;
	
}
.zeit {
	width:15%;
	margin: 10px 0px;
	float:left;
	text-align:center;
}
.vod {
	width:15%;
	float:left;
	text-align:center;
	margin: 10px 0px;
}
.derTipp {
	font-size:17px;
	font-weight:bold;
	color:#ba9056;
}
.vs {
	width:9%;
	float:left;
	margin: 10px 0px;
}
#headlist {
	border-bottom:2px solid #999;
	width:98%;
	clear:both;
	font-weight:bold;
	margin-right:20pt;
}
.weekhead {
	padding-top:15px;
}
#weekgames {
	width:98%;
	text-align:center;
	padding: 5px 0px;
}
.weekselect {
	margin: 15px 0px;
	margin-top:30px;

}
