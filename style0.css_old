}
.dW {
	border:3px solid #099acb;
	background:none;
}
.win {
	/*border: 1px solid green;*/	
}
.lose {
	/*border: 1px solid red;*/
}
#gamewrap {
	width:100%;
	clear:both;
	border-bottom:1px solid #4f4f4f;
}
.locked {
	cursor:auto;
}
#rank {
 float:left;
 width:8%;
 padding-top:5px;
}
#winel {
 float:left;
 width:23%;
 padding-top: 5px;
}
#winline {
 clear:both;
 width:100%;
 height:30px;
 
}
#successTip {
	background:#099acb;
	-webkit-transition: all 0.4s;
    -moz-transition: all 0.4s;
	-o-transition: all 0.4s;
	transition: all 0.4s;
	position:absolute;
	left:45%;
	height:25px;
	width:200px;
	top:2%;
	text-align:center;
}
#headerhalf{
	width:50%;
	float:left;
	height:50px;
}
#header{
	clear:both;
	height:43px;
}
#spoiler, #theme{
 background:#ba9056;
 height:30px;
 width:120px;
 float:right;
}
.spoilerChoose {
 background:#099acb;
 height:22px;
 width:58px;
 margin:1px;
 padding:3px 0px;
 text-align: center;
}
.spoilerTxt {
 margin:3px;
 padding-top:8px;
 line-height:2;
 vertical-align:text-bottom;
}
.option {
 float:right;
 margin-right:10px;
 width:40%;
 text-align:left;
}
.welcome {
	color:#fff;
	line-height:2;
